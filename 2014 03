#include<iostream>
#include<math.h>
#include<stdlib.h>
using namespace std;
int main(){
	int n=0 ,y=0 ,i=0;
	cin >> n;
	if(n>=0&&n<=10000){
		y= (n*n)+n+41;
		for(i=2;i<=y;i++)
		{
			if(y%i==0)
				break;	
		}
		if(i>=y)
			cout << "質數" << endl;	
		else
			cout << "非質數" << endl;
	}
	system("PAUSE");
	return 0;
}
